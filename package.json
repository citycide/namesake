{
  "name": "namesake",
  "version": "1.0.0",
  "description": "Find available & relevant npm package names for your project.",
  "main": "index.js",
  "author": {
    "name": "Bo Lingen",
    "email": "thecitycide@gmail.com",
    "url": "github.com/citycide"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/citycide/namesake.git"
  },
  "homepage": "https://github.com/citycide/namesake",
  "bugs": "https://github.com/citycide/namesake/issues",
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "name",
    "finder",
    "utility",
    "authors",
    "maintainers",
    "lightscript"
  ],
  "files": [
    "index.js"
  ],
  "scripts": {
    "build": "babel src -d . -x .lsc",
    "watch": "npm run build -- -w",
    "pretest": "npm run build",
    "test": "ava",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "datamuse": "^1.0.5",
    "got": "^6.7.1",
    "just-kebab-case": "^1.0.0",
    "npm-name": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.0",
    "babel-plugin-lightscript": "^0.5.1",
    "babel-preset-env": "^1.4.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          }
        }
      ]
    ],
    "plugins": [
      "lightscript"
    ]
  }
}
