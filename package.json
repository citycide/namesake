{
  "name": "namesake",
  "version": "3.1.0",
  "description": "Find available & relevant npm package names for your project.",
  "main": "index.js",
  "author": "Bo Lingen <lingenbw@gmail.com> (https://github.com/citycide)",
  "license": "MIT",
  "repository": "https://github.com/citycide/namesake.git",
  "homepage": "https://github.com/citycide/namesake",
  "bugs": "https://github.com/citycide/namesake/issues",
  "engines": {
    "node": ">=10"
  },
  "exports": {
    "import": "./index.mjs",
    "require": "./index.js"
  },
  "keywords": [
    "name",
    "finder",
    "utility",
    "authors",
    "maintainers"
  ],
  "files": [
    "cache.js",
    "index.js",
    "index.d.ts",
    "index.mjs",
    "index.mjs.d.ts"
  ],
  "scripts": {
    "lint": "standardx \"*.{js,mjs}\" | snazzy",
    "test": "ava",
    "prepublishOnly": "npm run lint && npm test"
  },
  "dependencies": {
    "datamuse": "^1.0.5",
    "got": "^11.3.0",
    "npm-name": "^6.0.0",
    "slugify": "^1.4.0"
  },
  "devDependencies": {
    "ava": "^3.9.0",
    "date-fns": "^2.14.0",
    "snazzy": "^8.0.0",
    "standardx": "^5.0.0"
  }
}
